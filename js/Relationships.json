{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/GoSearchGoole/FIT3179-Data-Visualization-2/main/data/Relationships.csv"
  },
  "repeat": {"row": ["Life Expectancy", "CO2 Emissions", "Health Expenditure %", "Adult Mortality", "Infant Deaths"]},
  "spec": {
    "mark": "point",
    "encoding": {
      "x": {"field": {"repeat": "row"}, "type": "quantitative"},
      "y": {"field": {"repeat": "column"}, "type": "quantitative"}
    }
  }
}
